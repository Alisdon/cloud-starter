/* <origin src="https://raw.githubusercontent.com/BriteSnow/cloud-bigapp/master/frontends/web/src/web-components/c-ico-symbol.pcss" /> */

c-ico, c-symbol{
	/* making sure that parent don't change value case, which is used as asset name */
	text-transform: none; 
	padding: 0;
	margin: 0;
	width: 1.5rem;
	height: 1.5rem;
	display: flex;
	align-items: center;
	justify-content: center;
	fill: #aaa;
	svg.symbol{
		width: 100%;
		height: 100%;
	}
}